CREATE TABLE prenotazione (
    nome VARCHAR(20),
    descrizione VARCHAR(255),
    dipendente VARCHAR(20),
    data_inizio DATETIME,
    data_fine DATETIME,
    prezzo DOUBLE,
    sede VARCHAR(20),
    id INT NOT NULL AUTO_INCREMENT,
    PRIMARY KEY (id)
);


create table dipendente(
    nome varchar(20),
    sede varchar(20)
)

CREATE TABLE merci(
    ID VARCHAR(40) PRIMARY KEY,
    nome VARCHAR(100),
    descrizione TEXT,
    quantita INT,
    prezzo DECIMAL(10, 2),
    data_carico TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    data_scarico TIMESTAMP
);

CREATE TABLE spese(
    Id int not null AUTO_INCREMENT,
    nome varchar(50),
    prezzo double,
    data date,
    primary key (id)
);